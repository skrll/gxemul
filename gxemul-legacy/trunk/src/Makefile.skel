CFLAGS=$(CWARNINGS) $(COPTIM) $(XINCLUDE) $(INCLUDE)

OBJS=main.o \
	emul.o emul_parse.o file.o debugger.o diskimage.o \
	net.o machine.o symbol.o x11.o cpu.o \
	cpu_mips.o coproc.o mips16.o memory_fast_v2h.o useremul.o bintrans.o \
	cpu_ppc.o \
	dec_prom.o ps2_bios.o arcbios.o


all: $(OBJS)

$(OBJS): Makefile

bintrans.o: bintrans.c bintrans_alpha.c bintrans_i386.c bintrans_sparcv9.c

cpu_mips.o: cpu_mips.c memory.c memory_v2p.c

clean:
	rm -f $(OBJS) *core

clean_all: clean
	rm -f Makefile

