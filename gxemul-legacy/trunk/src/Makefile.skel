CFLAGS=$(CWARNINGS) $(COPTIM) $(XINCLUDE) $(INCLUDE)
OBJS=main.o emul.o file.o machine.o \
	diskimage.o net.o \
	bintrans.o useremul.o \
	cpu.o coproc.o mips16.o symbol.o \
	x11.o \
	dec_prom.o ps2_bios.o arcbios.o

all: $(OBJS)

$(OBJS): Makefile

bintrans.o: bintrans.c bintrans_alpha.c bintrans_i386.c

cpu.o: cpu.c memory.c

clean:
	rm -f $(OBJS) *core

clean_all: clean
	rm -f Makefile

