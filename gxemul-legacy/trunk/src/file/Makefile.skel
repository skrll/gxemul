CFLAGS=$(CWARNINGS) $(COPTIM) $(XINCLUDE) $(DINCLUDE)

OBJS=file.o

all: $(OBJS)
	../make_index.sh

$(OBJS): Makefile

file.o: file.c file_aout.c file_ecoff.c file_elf.c file_macho.c \
	file_raw.c file_srec.c


clean:
	rm -f $(OBJS) *core

clean_all: clean
	rm -f Makefile

